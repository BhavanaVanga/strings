#include<bits/stdc++.h>
using namespace std;

int main(){
   // write your code here
   int t;
   cin>>t;
   while(t--)
   {
       string a,b;
       cin>>a>>b;
       sort(a.begin(),a.end());
       sort(b.begin(),b.end());
       if(a==b)
        cout<<"True\n";
       else
        cout<<"False\n";
   }
    return 0;
}
