#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    getline(cin,s);
    int n=s.length();
    string str="";
    for(int i=0;i<n;i++)
    {
        s[i]=tolower(s[i]);
        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u')
          continue;
        else
          str+=s[i];
    }
    cout<<str;
    return 0;
}
